<div class="grid_6">
	<h3>Project settings</h3>
<%= form_for(@project) do |f| %>
  <p>
    <%= f.label :name %><br/>
    <%= f.text_field :name %>
  </p>
  <p>
    <%= f.label :permalink %><br/>
    <%= f.text_field :permalink %>
  </p>
  <p>
    <%= f.submit "Save settings" %>
  </p>
<%end%>
</div>
